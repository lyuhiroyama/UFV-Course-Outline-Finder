{
  "manifest_version": 2,
  "name": "UFV Course Outline Finder",
  "version": "1.0.0",
  "icons": {"128": "assets/logo_128x128.png"},
  "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["popup.js"]
}],
"browser_action": {
  "default_popup": "main/explorer.html",
  "default_icon": "assets/logo_128x128.png",
  "default_title": "UFV Course Outline Finder"
}
}