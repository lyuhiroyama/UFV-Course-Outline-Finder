{
  "manifest_version": 3,
  "name": "UFV Course Outline Finder",
  "version": "1.0.0",
  "icons": {"128": "ufv-logo1.png"},
  "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["popup.js"]
}],
"action": {
  "default_popup": "popup.html",
  "default_icon": "ufv-logo1.png",
  "default_title": "UFV Course Outline Finder"
}
}